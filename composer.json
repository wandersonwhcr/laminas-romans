{
    "name": "wandersonwhcr/laminas-romans",
    "description": "Laminas Project Romans Integration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laminas",
        "laminas-project",
        "laminas-filter",
        "laminas-validator",
        "laminas-hydrator",
        "laminas-framework",
        "roman",
        "roman-number",
        "roman-numeral",
        "converter",
        "format"
    ],
    "autoload": {
        "psr-4": {
            "Zend\\Romans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Romans\\": "test"
        }
    },
    "require": {
        "php": "^7.0",
        "wandersonwhcr/romans": "^1.2",
        "zendframework/zend-filter": "2.7.*",
        "zendframework/zend-hydrator": "2.2.*",
        "zendframework/zend-validator": "2.9.*",
        "zendframework/zend-view": "2.9.*"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "1.2.*",
        "phpmd/phpmd": "2.6.*",
        "phpunit/phpunit": "6.1.*",
        "sebastian/phpcpd": "3.0.*",
        "squizlabs/php_codesniffer": "2.8.*",
        "zendframework/zend-modulemanager": "2.7.*",
        "zendframework/zend-mvc": "3.0.*",
        "zendframework/zend-servicemanager": "3.3.*"
    },
    "suggest": {
        "zendframework/zend-modulemanager": "Needed to load this package as Zend Framework module",
        "zendframework/zend-servicemanager": "Allows the use of service factories into Zend Framework"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "optimize": [
            "composer install --no-dev --optimize-autoloader --classmap-authoritative"
        ],
        "test": [
            "parallel-lint src",
            "phpunit",
            "phpcpd src",
            "phpmd src text phpmd.xml",
            "phpcs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}

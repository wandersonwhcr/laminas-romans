{
    "name": "wandersonwhcr/laminas-romans",
    "description": "Laminas Project Romans Integration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laminas",
        "laminas-project",
        "laminas-filter",
        "laminas-validator",
        "laminas-hydrator",
        "laminas-framework",
        "roman",
        "roman-number",
        "roman-numeral",
        "converter",
        "format"
    ],
    "autoload": {
        "psr-4": {
            "Laminas\\Romans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Romans\\": "test"
        }
    },
    "require": {
        "php": "^7.0",
        "wandersonwhcr/romans": "1.3.*",
        "zendframework/zend-view": "2.11.*",
        "laminas/laminas-filter": "2.11.*",
        "laminas/laminas-hydrator": "4.1.*",
        "laminas/laminas-validator": "2.14.*"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "1.2.*",
        "phpmd/phpmd": "2.9.*",
        "phpunit/phpunit": "9.5.*",
        "sebastian/phpcpd": "6.0.*",
        "squizlabs/php_codesniffer": "3.5.*",
        "laminas/laminas-mvc": "3.2.*",
        "laminas/laminas-modulemanager": "2.10.*",
        "laminas/laminas-servicemanager": "3.6.*"
    },
    "suggest": {
        "laminas/laminas-modulemanager": "Needed to load this package as Laminas module",
        "laminas/laminas-servicemanager": "Allows the use of service factories into Laminas"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "optimize": [
            "composer install --no-dev --optimize-autoloader --classmap-authoritative"
        ],
        "test": [
            "parallel-lint src",
            "phpunit",
            "phpcpd src",
            "phpmd src text phpmd.xml",
            "phpcs"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
